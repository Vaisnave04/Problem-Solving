/*Using If-else*/

#include<stdio.h>
int main(){
    int num1=13,num2=8;
    if(num1>num2){
        printf("%d is greater",num1);
    }
    else{
        printf("%d is greater",num2);
    }
}

/*Using ternary operator*/
#include<stdio.h>
int main(){
    int num1=1,num2=8;
    (num1>num2)?printf("%d is greater",num1):printf("%d is greater",num2);
}
