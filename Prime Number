#include<stdio.h>
#include<math.h>
int main(){
    int num=13;
    int isprime=1;
    if(num<2){
        printf("%d is not a prime number",num);
    }
    else{
        for(int i=2;i<=sqrt(num);i++){
            if(num%i==0){
                isprime=0;
            }
            
        }
    }
    if(isprime){
        printf("%d is a prime number",num);
    }
    else{
        printf("%d is not a prime number",num);
    }
}
