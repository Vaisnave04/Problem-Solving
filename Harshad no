#include<stdio.h>
int hars_no(int num){
    int sum=0;
    while(num!=0){
        
        sum=sum+num%10;
        num=num/10;
    }
    return sum;
}
int main(){
    int num=99,harsno;
    harsno=hars_no(num);
    if(num%harsno==0){
        printf("%d is a hars_no",num);
    }
    else{
        printf("%d is not a hars_no",num);
        
    }
    
    
}
