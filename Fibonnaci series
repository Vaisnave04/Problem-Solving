/*Without function*/
#include<stdio.h>
int main(){
    int a=0,b=1,n=5,sum;
    for(int i=a;i<=n;i++){
        sum=a+b;
        printf("%d\n",sum);
        a=b;
        b=sum;
        
    }
    printf("%d",sum);
    
}
/*Using Function*/

/*Using Function*/
#include<stdio.h>

int getFib(int n){
    
    if(n <= 1)
        return n;
        
    return getFib(n-1) + getFib(n-2);
    
}

int main()
{
    int n = 10;

    for(int i = 0; i < n; i++)
        printf("%d ",getFib(i));

    return 0;
}
