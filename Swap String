#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main(){
    char str1[]="geeks";
    char str2[]="forgeeks";
    char *temp=malloc(strlen(str1) + 1);;
    strcpy(temp,str1);
    strcpy(str1,str2);
    strcpy(str2,temp);
    printf("%s str1,%s str2",str1,str2);
    free(temp);
    getchar();
    return 0;
}
